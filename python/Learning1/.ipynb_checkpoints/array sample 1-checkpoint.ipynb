    "#!/usr/bin/env python3\n",
    "arr = [10, -2, -44, 5, 55, 44, 128, -200]\n",
    "max = arr[0]\n",
    "print(max)\n",
    "for x in arr:\n",
    "    if x > max:\n",
    "       max = x\n",
    "print(\"Maximum element in the array\", max)\n",
    "print(\"Length of array is\", len(arr))\n"
